        "dotenv": "^16.0.2",
        "fs-extra": "^11.1.0",
        "get-port-please": "^3.0.1",
        "winston": "^3.8.1"
    },
    "devDependencies": {
        "eslint": "^8.23.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0"
    },
    "bin": {
        "screeps-grafana": "./bin/setup.js"
    },
    "files": [
        "src",
        "conf",
        "dashboards",
        "docker-compose.example.yml",
        "example.env",
        ".dockerignore"
