        "start-private": "node src/setup/index.js --grafanaType=private --grafanaPort 3000",
        "start-private-test": "node src/setup/index.js --grafanaType=private --grafanaPort=3000 --force",
        "lint": "eslint **/*",
        "lint-fix": "eslint **/* --fix"
