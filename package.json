{
    "name": "screeps-grafana",
    "version": "2.5.6",
    "scripts": {
        "start:mmo": "node src/setup/index.js --grafanaType=mmo --grafanaPort=3000",
        "start:private": "node src/setup/index.js --grafanaType=private --grafanaPort=3000",
        "start:private-test": "node src/setup/index.js --grafanaType=private --grafanaPort=3000 --force --debug --includePushStatusApi",
        "start:standalone": "node src/setup/index.js --force --disablePushGateway --grafanaPort=3000 --relayPort=2003",
        "start:standalone-traefik": "node src/setup/index.js --traefik --force --disablePushGateway --grafanaPort=3000 --relayPort=2003 --grafanaDomain=grafana.localhost",
        "lint": "eslint src/**/*.js && eslint dashboards/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix && eslint dashboards/**/*.js --fix",
        "update-stats-getter": "docker-compose up --detach --build"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "dotenv": "^16.0.2",
        "fs-extra": "^11.1.0",
        "get-port-please": "^3.0.1",
        "minimist": "^1.2.7",
        "winston": "^3.8.1"
    },
    "devDependencies": {
        "eslint": "^8.23.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0"
    },
    "bin": {
        "screeps-grafana": "./bin/setup.js"
    },
    "files": [
        "src",
        "grafanaConfig.example",
        "dashboards",
        "docker-compose.example.yml",
        "example.env",
        "users.example.json",
        ".dockerignore",
        "go-carbon-storage"
    ]
}
